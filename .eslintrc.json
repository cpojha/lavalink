{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true
    },
    "extends": "google",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "rules": {
        "object-curly-spacing": 0,
        "require-jsdoc": 0,
        "no-tabs": ["error", { "allowIndentationTabs": true }]
    }
}